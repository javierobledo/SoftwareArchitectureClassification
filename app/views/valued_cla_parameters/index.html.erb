<p id="notice"><%= notice %></p>

<h1>Valued Cla Parameters</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Parameter</th>
      <th>Classification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @valued_cla_parameters.each do |valued_cla_parameter| %>
      <tr>
        <td><%= valued_cla_parameter.value %></td>
        <td><%= valued_cla_parameter.parameter %></td>
        <td><%= valued_cla_parameter.classification %></td>
        <td><%= link_to 'Show', valued_cla_parameter %></td>
        <td><%= link_to 'Edit', edit_valued_cla_parameter_path(valued_cla_parameter) %></td>
        <td><%= link_to 'Destroy', valued_cla_parameter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Valued Cla Parameter', new_valued_cla_parameter_path %>
