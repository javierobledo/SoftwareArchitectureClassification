<p id="notice"><%= notice %></p>

<h1>Preprocessings</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Corpus</th>
      <th>Algorithm</th>
      <th>Parent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @preprocessings.each do |preprocessing| %>
      <tr>
        <td><%= preprocessing.id %></td>
        <td><%= preprocessing.corpus.name %></td>
        <td><%= preprocessing.algorithm.name %></td>
        <td><%= preprocessing.parent_id %></td>
        <td><%= link_to 'Show', preprocessing %></td>
        <td><%= link_to 'Edit', edit_preprocessing_path(preprocessing) %></td>
        <td><%= link_to 'Destroy', preprocessing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Preprocessing', new_preprocessing_path %>
